.nav {
  z-index: 99;
  pointer-events: none;
  width: 100%;
  top: 0;
  position: fixed;
}

.nav__bar {
  position: absolute;
  width: 100%;
  height: 12rem;
  background: #1452A3;
  transition: transform cubic-bezier(0.4, 0, 0.2, 1) 300ms, background ease-in-out 100ms;
}

.nav__left, .nav__right {
  position: absolute;
  top: 3rem;
  height: 6rem;
  display: flex;
  align-items: center;
  pointer-events: auto;
  transition: opacity 100ms linear;
}

.nav__left {
  left: calc(50% - 70rem);
}

.nav__right {
  right: calc(50% - 70rem);
}

.nav__toggle, .nav__cart {
  border: none;
  border-radius: 0;
  background: transparent;
  outline: none;
}

.nav__toggle {
  width: 5rem;
  height: 5rem;
  background: #ffffff;
  margin-right: 4rem;
  background-image: url({{ 'menu-icon.svg' | asset_url }});
  background-position: center;
  background-repeat: no-repeat;
  background-size: 45%;
  cursor: pointer;
}

.nav__toggle:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  height: 0;
  width: 0;
  opacity: 0;
  pointer-events: none;
  background-image: url({{ 'menu-close-icon.svg' | asset_url }});
}

.nav__button {
  width: 140px;
  height: 5rem;
  line-height: 4.5rem;
  font-size: 2rem;
  letter-spacing: 0.2rem;
  background: transparent;
  display: inline-block;
  font-family: "FoundersGroteskCond", sans-serif;
  text-transform: uppercase;
  border: 2px solid #ffffff;
  text-align: center;
  font-weight: 500;
}

.nav__button:hover .nav__buttonInner {
  color: #1452A3;
}

.nav__buttonInner {
  position: relative;
  transition: color 400ms ease-in-out;
}

.nav__cart {
  width: 5.1rem;
  height: 5.1rem;
  border-radius: 5.1rem;
  border: 2px solid #ffffff;
  background: transparent;
  margin-left: 4rem;
  cursor: pointer;
}

.nav__cart svg {
  position: absolute;
  top: 51%;
  left: 48%;
  transform: translate3d(-50%, -50%, 0);
  pointer-events: none;
  width: 55%;
}

.nav__cart svg .fill {
  transition: fill 300ms ease-in-out;
}

.nav__cart:hover svg .fill {
  fill: #1452A3;
  transition: fill 600ms ease-in-out;
}

.nav__cartIndicator {
  position: absolute;
  top: 0;
  right: 0;
  width: 3rem;
  height: 3rem;
  background: #56AADA;
  border-radius: 100%;
  transform: translate3d(25%, -25%, 0);
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 2rem;
  font-family: "FoundersGroteskCond", sans-serif;
  letter-spacing: 0.2rem;
  font-weight: 700;
  opacity: 0;
}

.nav__cartIndicator.show {
  opacity: 1;
}

.nav__brand {
  width: 21rem;
  height: 3.8rem;
}

.nav__brand svg {
  width: 100%;
  height: auto;
}

.nav__menu {
  transition: transform cubic-bezier(0.4, 0, 0.2, 1) 300ms;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  pointer-events: none;
}

.nav--hide .nav__bar {
  transform: translateY(-12.5rem);
  transition: transform cubic-bezier(0.2, 0, 0.4, 1) 300ms, background ease-in-out 10ms 300ms;
}

.nav--hide .nav__menu {
  transform: translateY(12.5rem);
  transition: transform cubic-bezier(0.2, 0, 0.4, 1) 300ms;
}

.nav--top .nav__bar {
  background: rgba(20, 82, 163, 0);
}

.nav--white .nav__bar {
  background: #ffffff;
}

.nav--white.nav--top .nav__bar {
  background: rgba(255, 255, 255, 0);
}

.nav--white .nav__brand svg .fill {
  fill: #1452A3;
}

.nav--white .nav__button, .nav--white .nav__cart {
  border-color: #1452A3;
}

.nav--white .nav__button .water__inner, .nav--white .nav__cart .water__inner {
  background: #1452A3;
}

.nav--white .nav__button .water__inner::before, .nav--white .nav__cart .water__inner::before {
  background-image: url({{ 'wave-blue.svg' | asset_url }});
}

.nav--white .nav__button .nav__buttonInner {
  color: #1452A3;
}

.nav--white .nav__button:hover .nav__buttonInner {
  color: #ffffff;
}

.nav--white .nav__cart svg .fill {
  fill: #1452A3;
}

.nav--white .nav__cart:hover svg .fill {
  fill: #ffffff;
}

.menu-open .nav--hide .nav__bar {
  transform: translateY(0);
  transition: transform cubic-bezier(0.4, 0, 0.2, 1) 300ms, background ease-in-out 100ms;
}

.menu-open .nav--hide .nav__menu {
  transform: translateY(0);
  transition: transform cubic-bezier(0.4, 0, 0.2, 1) 300ms;
}

