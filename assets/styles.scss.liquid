  transform: translate3d(0, 0, 0);
}

.cart__close, .cart__info, .cart__cta {
  min-height: 5.8rem;
  max-height: 5.8rem;
  background: #D3E6F0;
  color: #1452A3;
  font-size: 2rem;
  font-family: "FoundersGroteskCond", sans-serif;
